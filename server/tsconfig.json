{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "outDir": "dist",
    "baseUrl": ".",
    "rootDir": "src",
    "lib": [
      "es2017",
      "esnext.asynciterable"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}