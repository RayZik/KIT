{
  "name": "KIT_server",
  "version": "0.2.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "dev": "rimraf ./dist && concurrently --raw \" npm run copy:api-watch \" \"npm run dev:server\" \"npm run dev:start\"",
    "copy:api": "cpx \"./src/api/**/*.{gql,graphql,proto,json}\" \"./dist/api\"",
    "copy:api-watch": "cpx \"./src/api/**/*.{gql,graphql,json}\" \"./dist/api\" --watch",
    "dev:server": "NODE_ENV=development tsc -w",
    "dev:start": "bash ./dev-server-star.sh",
    "build": "rm -Rf ./dist && tsc && npm run copy:api",
    "deploy": "npm run build &&  node .ghpages server"
  },
  "engines": {
    "node": "8.11.3",
    "npm": "6.2.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "ZIK",
  "license": "ISC",
  "homepage": "https://github.com/mralexrabota",
  "dependencies": {
    "apollo-server-express": "2.1.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cpx": "1.5.0",
    "express": "4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "graphql": "14.0.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "5.3.7",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "0.12.7"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "4.16.0",
    "@types/express-jwt": "0.0.40",
    "@types/express-session": "^1.15.11",
    "@types/graphql": "14.0.3",
    "@types/lodash": "4.14.117",
    "@types/mongoose": "^5.3.0",
    "@types/node": "10.12.0",
    "@types/passport": "^0.4.6",
    "@types/passport-local": "^1.0.33",
    "concurrently": "4.0.1",
    "gh-pages": "2.0.1",
    "lodash": "4.17.11",
    "nodemon": "1.18.4",
    "shelljs": "0.8.2",
    "typescript": "3.1.3"
  }
}
