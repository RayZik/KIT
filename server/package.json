    "apollo-server-express": "~2.0.6",
    "cpx": "~1.5.0",
    "express": "~4.16.3",
    "graphql": "~14.0.2",
    "mysql": "~2.16.0",
    "path": "~0.12.7"
  },
  "devDependencies": {
    "@types/cookie-parser": "~1.4.1",
    "@types/express": "~4.16.0",
    "@types/graphql": "~14.0.0",
    "@types/lodash": "^4.14.116",
    "@types/mysql": "~2.15.5",
    "@types/node": "~10.9.4",
