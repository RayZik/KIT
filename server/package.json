{
  "name": "shelter_server",
  "version": "0.2.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "dev": "rimraf ./dist && concurrently \"tsc -w \" \" npm run copy:api-watch \" \"nodemon ./dist/index.js\" ",
    "copy:api": "cpx \"./src/api/**/*.{gql,graphql,proto,json}\" \"./dist/api\"",
    "copy:api-watch": "cpx \"./src/api/**/*.{gql,graphql,json}\" \"./dist/api\" --watch",
    "build": "rm -Rf ./dist && tsc && npm run copy:api",
    "deploy": "npm run build &&  node .ghpages server"
  },
  "engines": {
    "node": "8.11.3",
    "npm": "6.2.0"
  },
