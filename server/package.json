{
  "name": "KIT_server",
  "version": "0.3.0",
  "description": "",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "rimraf ./dist && concurrently --raw \" npm run copy:api-watch \" \"npm run dev:server\" \"npm run dev:start\"",
    "copy:api": "cpx \"./src/api/**/*.{gql,graphql,json}\" \"./dist/api\"",
    "copy:api-watch": "cpx \"./src/api/**/*.{gql,graphql,json}\" \"./dist/api\" --watch",
    "dev:server": "NODE_PATH=src NODE_ENV=development tsc -w",
    "dev:start": "bash ./scripts/dev-server-starter.sh",
    "build": "rm -Rf ./dist && tsc && npm run copy:api",
    "deploy": "npm run build &&  node ./scripts/.ghpages server"
  },
  "engines": {
    "node": "8.11.3",
    "npm": "6.2.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "ZIK",
  "license": "ISC",
  "homepage": "https://github.com/mralexrabota",
  "dependencies": {
    "apollo-server-express": "2.4.8",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.4",
    "express": "4.16.4",
    "express-jwt": "5.3.1",
    "graphql": "14.1.1",
    "graphql-resolvers": "0.3.2",
    "jsonwebtoken": "8.5.0",
    "mongoose": "5.4.16",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.1",
    "@types/express": "4.16.1",
    "@types/express-jwt": "0.0.40",
    "@types/graphql": "14.0.3",
    "@types/graphql-resolvers": "^0.2.0",
    "@types/jsonwebtoken": "8.3.0",
    "@types/lodash": "4.14.118",
    "@types/mongoose": "5.3.11",
    "@types/node": "10.12.5",
    "@types/uuid": "^3.4.4",
    "cpx": "1.5.0",
    "concurrently": "4.1.0",
    "gh-pages": "2.0.1",
    "lodash": "4.17.11",
    "nodemon": "1.18.10",
    "typescript": "3.3.3333"
  }
}
